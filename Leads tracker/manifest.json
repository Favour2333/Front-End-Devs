{
  "manifest_version": 3,
  "name": "Leads Tracker",
  "version": "1.0",
  "description": "Chrome Extension for Tracking Leads",
  "icons": {
    "48": "images/icon.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  },
  "permissions": [
    "tabs"
  ]
}
